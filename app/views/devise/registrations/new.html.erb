<section class="form-auth text-center">

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>


    <h1 class="h3 mb-3 font-weight-normal">Please sign up</h1>    
    <%= render "devise/shared/error_messages", resource: resource %>

      <%= f.text_field :first_name, autofocus: true, class: "form-control", placeholder: "First name" %>

      <%= f.text_field :last_name,  class: "form-control", placeholder: "Last name" %>

      <%= f.email_field :email, autofocus: true, class: "form-control", id: "inputEmail", placeholder: "Email address", autocomplete: "email" %>

      <%= f.password_field :password, autocomplete: "new-password", class: "form-control", placeholder: "Password" %>




      <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-control", placeholder: "Password confirmation" %>


    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign up</button>
    <p class="mt-5 mb-3 text-muted">Â© 2017-2020</p>
  <% end %>

  <%= render "devise/shared/links" %>

</section>
